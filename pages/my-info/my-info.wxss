  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.projLogo {
  margin-top: 10px;
  width: 70px;
  height: 70px;
}
.title{
  font-family: Yu Mincho;
  margin-bottom: 10px;
}
.myBody{
  height: 100%;
  justify-content: space-between;
  align-content: center;
}
.ownBut{
  margin-top: 20px;
  margin-bottom: 20px;
  height: 70px;
  width: 70%;
  align-content: center;
  justify-content: center;
  align-items: center;
  display: flex;
  box-shadow: 3px 3px 3px rgb(138, 138, 138);
  font-size: 22px;
  font-family: 'Microsoft JhengHei';
