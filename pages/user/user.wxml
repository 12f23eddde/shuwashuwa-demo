<view class="container">
  <van-cell-group>
    <van-field model:value="{{ userName }}" label="姓名"/>
    <van-field model:value="{{ studentId }}" label="学号"/>
    <van-field model:value="{{ phoneNumber }}" label="手机号"/>
    <van-field model:value="{{ nickName }}" label="昵称"/>
    <van-field model:value="{{ identity }}" label="身份"/>
    <van-field model:value="{{ grade }}" label="年级"/>
    <van-field model:value="{{ email }}" label="邮箱"/>
    <van-field model:value="{{ department }}" label="部门"/>
    <van-field model:value="{{ comment }}" label="备注"/>
  </van-cell-group>
  <van-button type="primary" bindtap="onSubmit"> 提交 </van-button>
  <tab-bar active='3'></tab-bar>
</view>
